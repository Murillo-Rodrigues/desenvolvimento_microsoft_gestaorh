@model List<Prontuario>

<!DOCTYPE html>
<html>

<head>
    <title>Gerenciamento de Prontuários</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
          integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous" />
    <link href="/css/site.css" rel="stylesheet" />
</head>

<body>
    <h2>Gerenciamento de Prontuários</h2>
    <a asp-action="Index" asp-controller="Funcionario">Tabela Funcionários</a><br />
    <a asp-action="Index" asp-controller="Caixa">Tabela Caixas</a><br />
    <a asp-action="Index" asp-controller="Prontuario">Tabela Prontuários</a><br />
    <a asp-action="Index" asp-controller="Solicitacao">Tabela Solicitações</a><br /><br />

    <a asp-action="Cadastrar" asp-controller="Funcionario">Cadastrar Funcionário</a><br />
    <a asp-action="Cadastrar" asp-controller="Caixa">Cadastrar Caixa</a><br />
    <a asp-action="Cadastrar" asp-controller="Prontuario">Cadastrar Prontuario</a><br />
    <a asp-action="Cadastrar" asp-controller="Solicitacao">Cadastrar Solicitação</a><br /><br />
    <fieldset>

        <div id="prontuarios" class="row">
            @foreach (Prontuario prontuarioCadastrado in Model)
            {
<div class="col-sm-3 div-conteudo">
    <span><b>Matrícula: </b>@prontuarioCadastrado.Funcionario.Matricula</span><br />
    <span><b>Custódia: </b>@prontuarioCadastrado.Caixa.Custodia</span>
</div>}
        </div>
    </fieldset>


</body>
</html>
